<% if current_page.first? %>
  <li class="disabled">
    <%= content_tag :a do %>
      <i class="fa fa-angle-left "></i>
    <% end %>
  </li>
<% else %>
  <li class="waves-effect">
    <%= link_to url, :rel => 'next', :remote => remote do %>
      <i class=" fa fa-angle-left material-icons"></i>
    <% end %>
  </li>
<% end %>
